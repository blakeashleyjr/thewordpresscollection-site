{
  "name": "blakeashleyjr/thewordpresscollection-site",
  "type": "project",
  "repositories": {
    "wp-packagist": {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
    },
    "ba-satispress": {
      "type": "composer",
      "url": "https://app-65488fe2c1ac18543cd0d64b.closte.com/satispress/"
    }
  },
  "require": {
    "php": ">=8.3",
    "composer/installers": "^2.2",
    "vlucas/phpdotenv": "^5.5",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": ">=6.5.2",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.1.0",
    "wpackagist-theme/twentytwentyfour": "*",
    "ba-satispress/all-in-one-seo-pack-pro": "*",
    "ba-satispress/perfmatters": "*",
    "ba-satispress/searchwp": "*",
    "ba-satispress/searchwp-live-ajax-search": "*",
    "ba-satispress/stackable-ultimate-gutenberg-blocks-premium": "*",
    "wpackagist-plugin/wordfence-login-security": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.1",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
